{
  "name": "blckbook-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:stats": "ng build --stats-json",
    "build:browser:prod": "ng build --output-hashing=all",
    "build:prod": "npm run build:browser:prod && npm run build:server:prod",
    "server": "node local.js",
    "lint": "ng lint",
    "analyze": "webpack-bundle-analyzer dist/blckbook-ui/stats.json",
    "explore": "ng build --source-map && source-map-explorer dist/**/*.js",
    "deploy:prod": "ng build --configuration production --output-hashing=all && serverless deploy --stage prod",
    "deploy:dev": "ng build --configuration devDeployment --output-hashing=all && serverless deploy --stage dev"
  },
  "private": true,
  "dependencies": {
    "@airgap/beacon-sdk": "^3.1.3",
    "@angular/animations": "^14.1.3",
    "@angular/cdk": "^14.1.3",
    "@angular/common": "^14.1.3",
    "@angular/compiler": "^14.1.3",
    "@angular/core": "^14.1.3",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.1.3",
    "@angular/material": "^14.1.3",
    "@angular/platform-browser": "^14.1.3",
    "@angular/platform-browser-dynamic": "^14.1.3",
    "@angular/router": "^14.1.3",
    "@asymmetrik/ngx-leaflet": "^14.0.1",
    "@aws-amplify/ui-angular": "^2.4.18",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "1.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@taquito/beacon-wallet": "^13.0.1",
    "@taquito/taquito": "^13.0.1",
    "@taquito/tzip16": "^13.0.1",
    "@vendia/serverless-express": "^4.3.11",
    "angular-translate-csv": "^0.8.3",
    "aws-amplify": "^4.3.33",
    "cors": "^2.8.5",
    "crypto-browserify": "^3.12.0",
    "dinero.js": "^1.9.1",
    "exifr": "^7.1.3",
    "leaflet": "^1.8.0",
    "leaflet-geosearch": "^3.6.1",
    "masonry-layout": "^4.2.2",
    "ngx-countdown": "^14.0.0",
    "ngx-masonry": "^13.0.0",
    "path": "^0.12.7",
    "rxjs": "^7.5.6",
    "serverless-api-compression": "^1.0.1",
    "serverless-finch": "^4.0.0",
    "urlencode": "^1.1.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.3",
    "@angular-eslint/builder": "^14.0.3",
    "@angular-eslint/eslint-plugin": "^14.0.3",
    "@angular-eslint/eslint-plugin-template": "^14.0.3",
    "@angular-eslint/schematics": "^14.0.3",
    "@angular-eslint/template-parser": "^14.0.3",
    "@angular/cli": "^14.1.3",
    "@angular/compiler-cli": "^14.1.3",
    "@types/compression": "^1.7.1",
    "@types/dinero.js": "^1.9.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.3.0",
    "@types/leaflet": "^1.7.11",
    "@types/node": "^12.11.1",
    "@types/url-parse": "^1.4.8",
    "@types/urlencode": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "source-map-explorer": "^2.5.2",
    "ts-loader": "^9.2.7",
    "typescript": "~4.7.4",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "serverless": "^3.22.0"
  }
}
