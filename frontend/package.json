    "@angular/core": "~13.2.4",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~13.2.4",
    "@angular/material": "^13.2.4",
    "@angular/platform-browser": "~13.2.4",
    "@angular/platform-browser-dynamic": "~13.2.4",
    "@angular/router": "~13.2.4",
    "@asymmetrik/ngx-leaflet": "^13.0.2",
    "@aws-amplify/auth": "^4.4.2",
    "@aws-amplify/ui-angular": "^2.1.3",
    "@aws-amplify/interactions": "^4.0.33",
    "@aws-amplify/storage": "^4.4.16",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "1.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@taquito/taquito": "11.2.0",
    "@taquito/tzip16": "11.2.0",
    "angular-translate-csv": "^0.8.3",
    "cors": "^2.8.5",
