{
  "name": "blckbook-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:stats": "ng build --stats-json",
    "build:browser:prod": "ng build --output-hashing=all",
    "build:prod": "npm run build:browser:prod && npm run build:server:prod",
    "server": "node local.js",
    "lint": "ng lint",
    "analyze": "webpack-bundle-analyzer dist/blckbook-ui/stats.json",
    "explore": "ng build --source-map && source-map-explorer dist/**/*.js",
    "build:server:serverless": "webpack --config webpack.server.config.js --progress",
    "deploy:prod": "ng build --configuration production --output-hashing=all && npm run build:server:serverless && serverless deploy --stage prod",
    "deploy:dev": "ng build --configuration devDeployment --output-hashing=all && npm run build:server:serverless && serverless deploy --stage dev"
  },
  "private": true,
  "dependencies": {
    "@airgap/beacon-sdk": "^2.3.9",
    "@angular/animations": "~13.2.4",
    "@angular/cdk": "~13.2.4",
